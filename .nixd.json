{
  "nixpkgs": {
   "expr": "import (builtins.getFlake \".#\").inputs.nixpkgs { }   "
  },
  "formatting": {
    "command": [ "nixfmt" ]
  },
  "options": { 
    "nixos": {  
      "expr": "(builtins.getFlake \".#\").neovimOptions"
    },
  },
  "diagnostic": {
    "suppress": [
      "sema-extra-with"
    ]
  }
}
